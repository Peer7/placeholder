{
  "name": "react-native-app",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "jest-expo": "~27.0.0",
    "react-native-scripts": "1.14.0",
    "react-test-renderer": "16.3.1"
  },
  "main": "./node_modules/react-native-scripts/build/bin/crna-entry.js",
  "scripts": {
    "start": "react-native-scripts start",
    "eject": "react-native-scripts eject",
    "android": "react-native-scripts android",
    "ios": "react-native-scripts ios",
    "build:docs": "../../node_modules/.bin/jsdoc -c jsdoc.json",
    "test": "PH_ENV=test jest --forceExit",
    "test:coverage": "PH_ENV=test jest --coverage --forceExit",
    "test:debug": "PH_ENV=test node --inspect-brk ../../node_modules/.bin/jest --runInBand --watch",
    "test:watch": "PH_ENV=test jest --runInBand --watch",
    "test:watch:all": "PH_ENV=test jest --runInBand --watchAll",
    "lint": "eslint ./ --fix",
    "depcheck": "depcheck ./"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "exp": "^55.0.5",
    "expo": "^27.0.1",
    "react": "16.3.1",
    "react-native": "~0.55.2"
  }
}
